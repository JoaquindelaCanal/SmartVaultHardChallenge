<?xml version="1.0" encoding="utf-8"?>
<businessObject>
  <propertyGroup>
    <property name="Id" type="int" />
    <property name="ProviderName" type="string" />
    <property name="ClientId" type="string" />
    <property name="ClientSecret" type="string" />
    <property name="RedirectUri" type="string" />
    <property name="Scopes" type="string" />
    <property name="CreatedOn" type="System.DateTime" />
    <property name="LastUpdatedOn" type="System.DateTime" />
  </propertyGroup>
  <script>
    CREATE TABLE OAuthIntegration (
    Id INTEGER PRIMARY KEY,
    ProviderName TEXT,
    ClientId TEXT,
    ClientSecret TEXT,
    RedirectUri TEXT,
    Scopes TEXT,
    CreatedOn TEXT,
    LastUpdatedOn TEXT
    );
  </script>
</businessObject>